{
  "name": "modern-stack-portfolio-react-client",
  "version": "1.0.0",
  "private": false,
  "sideEffects": false,
  "main": ".next/server/index.js",
  "scripts": {
    "dev": "next",
    "build": "rm -fr .next && npm run generate && next build && npm run copy && npm run export",
    "export": "next export",
    "copy": "node public/copy",
    "generate": "node public/meta",
    "start": "next start",
    "start:server": "DISABLE_SERVERLESS=true NODE_ENV=production node .next/server",
    "test": "jest | echo 'https://github.com/testing-library/react-testing-library/issues/509'",
    "test:dev": "jest --config jest.config.js",
    "prepublishOnly": "rm -rf build && tsc || true",
    "now-build": "npm run build",
    "build:next": "npm run build && npm run build:server",
    "build:docker": "IS_DOCKER=true DISABLE_SERVERLESS=true npm run build && npm run build:server",
    "build:server:only": "DISABLE_SERVERLESS=true NODE_ENV=production next build",
    "build:storybook": "STORYBOOK_DISPLAY_WARNING=true DISPLAY_WARNING=true build-storybook -c ./ -c .storybook",
    "build:babel": "NODE_ENV=development BABEL_ENV=development npx babel build/dist --out-dir build/dist || true",
    "build:server": "npm run build && npm run build:server:only && tsc || true && tsc --project tsconfig.server.json || true",
    "install:workspace": "cd ../../ && npm run install && cd packages/client",
    "dev:webpack": "webpack-dev",
    "dev:storybook": "start-storybook -p 9001 -c .storybook",
    "dev:analyze": "SHOULD_ANALYZE_BUNDLE=true npm run start:server",
    "dev:server": "rm -fr dist && rm -fr .next && npm run build:server && npm run start:server",
    "git:push": "npm run fix:all && npm run test",
    "fix:all": "npm run fix:prettier && npm run lint:style",
    "fix:prettier": "prettier --write src/**/*.{ts,tsx} && prettier --write src/*.{ts,tsx}",
    "lint:style": "stylelint './src/**/*.(tsx|ts)' --config './stylelint.config.js'",
    "lint": "eslint '{src,pages,stories}/**/*.{ts,tsx}' --ignore-path .gitignore --quiet",
    "lint:fix": "npm run lint -- --fix",
    "prettier:write": "prettier --write './**/*.{js,ts,tsx,scss}' --ignore-path .gitignore",
    "prettier:check": "prettier --check './**/*.{js,ts,tsx,scss}' --ignore-path .gitignore",
    "fix": "npm run prettier:write && npm run lint:fix"
  },
  "removedDependencies": {
    "contentful": "8.5.6",
    "contentful-cli": "1.9.10",
    "@contentful/rich-text-html-renderer": "15.3.3",
    "web-vitals": "2.1.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.3.0",
    "styled-components": "5.3.0",
    "history": "5.0.0",
    "@types/history": "4.7.9",
    "idb": "6.1.2",
    "mkdirp": "1.0.4",
    "@types/mkdirp": "1.0.2",
    "@types/react-router": "5.1.16",
    "@types/react-router-dom": "5.1.8"
  },
  "dependencies": {
    "@apollo/client": "3.4.16",
    "idb-keyval": "6.0.1",
    "next": "11.1.2",
    "next-offline": "5.0.5",
    "node-fetch": "2.6.2",
    "pino": "6.13.3",
    "react": "alpha",
    "react-dom": "alpha"
  },
  "devDependencies": {
    "@babel/cli": "7.15.4",
    "@babel/core": "7.15.5",
    "@babel/plugin-proposal-decorators": "7.15.4",
    "@babel/plugin-syntax-jsx": "7.14.5",
    "@babel/plugin-syntax-object-rest-spread": "7.8.3",
    "@babel/preset-env": "7.15.6",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@next/bundle-analyzer": "11.1.2",
    "@storybook/addon-a11y": "6.3.8",
    "@storybook/addon-controls": "6.3.8",
    "@storybook/addon-essentials": "6.3.8",
    "@storybook/addon-graphql": "6.2.9",
    "@storybook/addon-knobs": "6.3.1",
    "@storybook/addon-storyshots": "6.3.8",
    "@storybook/addon-storysource": "6.3.8",
    "@storybook/react": "6.3.8",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.2.1",
    "@types/eslint": "7.28.0",
    "@types/express": "^4.17.13",
    "@types/gapi.client.tagmanager": "2.0.2",
    "@types/jest": "27.0.2",
    "@types/pino": "6.3.11",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "5.1.15",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "babel-loader": "8.2.2",
    "babel-plugin-styled-components": "1.13.2",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "danger": "10.6.6",
    "date-fns": "2.25.0",
    "eslint": "7.32.0",
    "eslint-config-next": "^12.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-i18n": "^2.1.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "express": "4.17.1",
    "fork-ts-checker-webpack-plugin": "6.2.10",
    "intersection-observer": "0.12.0",
    "jest": "27.2.0",
    "jest-cli": "27.2.0",
    "jest-styled-components": "7.0.5",
    "lighthouse": "https://github.com/GoogleChromeLabs/lighthousebot",
    "pino-pretty": "7.0.1",
    "prettier": "2.4.1",
    "raf": "3.4.1",
    "schema-dts": "0.9.0",
    "stylelint": "13.13.1",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-jest": "27.0.5",
    "tslib": "2.3.1",
    "typescript": "4.4.3",
    "typescript-styled-plugin": "0.18.1",
    "vercel": "23.1.2"
  },
  "folders": [
    "public",
    ".next",
    "server",
    "pages",
    "src"
  ],
  "license": "MIT",
  "description": "React client portion of the generic portfolio, written in the modern web stack, used on jameswiens.dev site",
  "homepage": "https://jameswiens.dev",
  "author": "James Wiens <james@jameswiens.dev> (https://jameswiens.dev)",
  "repository": {
    "type": "git",
    "url": "git@github.com:aretecode/modern-stack-portfolio-react.git"
  },
  "keywords": [
    "jameswiens",
    "james",
    "wiens",
    "dev",
    "aretecode",
    "portfolio",
    "react",
    "graphql",
    "typescript",
    "styled-components"
  ],
  "browserslist": [
    "last 1 chrome version"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "./**/*.{js,ts,tsx}": [
      "npm run lint"
    ],
    "./**/*.{js,ts,tsx,scss}": [
      "npm run prettier:write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "useTabs": false,
    "parser": "typescript",
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "eslintConfig": {
    "plugins": [
      "import",
      "prettier",
      "@typescript-eslint",
      "i18n",
      "jsx-a11y",
      "react"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.eslint.json",
      "ecmaVersion": 2021,
      "sourceType": "module",
      "ecmaFeatures": {
        "modules": true,
        "impliedStrict": true
      }
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "extends": [
      "plugin:jsx-a11y/recommended",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:import/typescript",
      "plugin:prettier/recommended",
      "plugin:@next/next/recommended"
    ],
    "rules": {
      "import/no-unresolved": "off",
      "eol-last": "error",
      "semi": [
        "error",
        "never"
      ],
      "no-multi-spaces": "off",
      "semi-spacing": "error",
      "key-spacing": [
        2
      ],
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "never"
        }
      ],
      "padded-blocks": [
        "error",
        "never"
      ],
      "keyword-spacing": [
        "error",
        {
          "before": true,
          "after": true
        }
      ],
      "max-len": [
        "warn",
        {
          "code": 80,
          "ignoreComments": true,
          "ignoreUrls": true,
          "ignoreTemplateLiterals": true,
          "ignoreRegExpLiterals": true,
          "ignoreStrings": true
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "array-bracket-spacing": [
        "error",
        "never"
      ],
      "quotes": [
        1,
        "single",
        {
          "allowTemplateLiterals": true,
          "avoidEscape": true
        }
      ],
      "indent": "off",
      "no-trailing-spaces": [
        "error",
        {
          "skipBlankLines": true
        }
      ],
      "space-infix-ops": "off",
      "space-before-blocks": [
        "error",
        {
          "functions": "always",
          "keywords": "always",
          "classes": "always"
        }
      ],
      "no-invalid-regexp": "warn",
      "no-fallthrough": "warn",
      "no-unreachable": "warn",
      "no-shadow-restricted-names": "error",
      "no-new-require": "error",
      "no-mixed-spaces-and-tabs": "error",
      "no-empty": "error",
      "no-undef": "off",
      "no-dupe-keys": "error",
      "no-empty-character-class": "error",
      "no-self-compare": "error",
      "no-plusplus": [
        "error",
        {
          "allowForLoopAfterthoughts": true
        }
      ],
      "valid-typeof": "error",
      "handle-callback-err": "error",
      "max-depth": [
        "warn",
        4
      ],
      "max-statements": [
        "error",
        50
      ],
      "function-paren-newline": "off",
      "brace-style": [
        "error",
        "1tbs",
        {
          "allowSingleLine": false
        }
      ],
      "curly": [
        "error",
        "multi-or-nest",
        "consistent"
      ],
      "import/no-extraneous-dependencies": "off",
      "arrow-spacing": [
        "error",
        {
          "before": true,
          "after": true
        }
      ],
      "space-in-parens": [
        "error",
        "never"
      ],
      "space-before-function-paren": "off",
      "func-style": [
        "error",
        "declaration",
        {
          "allowArrowFunctions": true
        }
      ],
      "arrow-body-style": "off",
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "no-return-assign": [
        "error",
        "always"
      ],
      "func-names": "off",
      "template-tag-spacing": [
        "error",
        "never"
      ],
      "func-call-spacing": "error",
      "no-param-reassign": "warn",
      "prefer-rest-params": "warn",
      "import/extensions": "off",
      "import/no-named-as-default": "off",
      "import/export": "error",
      "no-continue": "off",
      "no-loop-func": "error",
      "camelcase": "off",
      "new-cap": "off",
      "id-length": [
        "warn",
        {
          "min": 2,
          "properties": "always",
          "exceptions": [
            "x",
            "i",
            "R",
            "_"
          ]
        }
      ],
      "id-blacklist": [
        "error",
        "err",
        "e",
        "_cb",
        "callback"
      ],
      "object-shorthand": [
        "warn",
        "always",
        {
          "avoidQuotes": true
        }
      ],
      "valid-jsdoc": [
        "off"
      ],
      "no-case-declarations": "off",
      "strict": "off",
      "no-underscore-dangle": "off",
      "no-mixed-requires": "off",
      "no-process-exit": "off",
      "no-warning-comments": "off",
      "no-confusing-arrow": "off",
      "require-jsdoc": "off",
      "class-methods-use-this": "off",
      "no-process-env": "off",
      "prefer-reflect": "off",
      "nonblock-statement-body-position": "off",
      "prefer-destructuring": "off",
      "prefer-template": "off",
      "global-require": "off",
      "no-invalid-this": "off",
      "prefer-const": "error",
      "no-undefined": "off",
      "prefer-spread": "off",
      "no-implicit-coercion": "off",
      "no-bitwise": "off",
      "no-use-before-define": "off",
      "import/no-duplicates": "off",
      "no-duplicate-imports": "off",
      "import/no-namespace": "off",
      "react/prop-types": "off",
      "react/display-name": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-namespace": "off",
      "@typescript-eslint/no-empty-function": "warn",
      "@typescript-eslint/no-unused-vars": "off",
      "@typescript-eslint/no-inferrable-types": "off",
      "@typescript-eslint/no-var-requires": "off",
      "@typescript-eslint/naming-convention": [
        "error",
        {
          "selector": "interface",
          "format": [
            "PascalCase"
          ]
        },
        {
          "selector": "typeAlias",
          "format": [
            "PascalCase"
          ]
        },
        {
          "selector": [
            "enumMember"
          ],
          "format": [
            "PascalCase"
          ]
        },
        {
          "selector": [
            "enum"
          ],
          "format": [
            "PascalCase"
          ],
          "custom": {
            "regex": "[A-Z]Kind$",
            "match": false
          }
        },
        {
          "selector": "variable",
          "types": [
            "boolean"
          ],
          "format": [
            "PascalCase",
            "UPPER_CASE"
          ],
          "leadingUnderscore": "allowSingleOrDouble",
          "trailingUnderscore": "allowSingleOrDouble",
          "prefix": [
            "is",
            "should",
            "has",
            "can",
            "did",
            "will",
            "IS",
            "SHOULD",
            "HAS",
            "CAN",
            "DID",
            "WILL"
          ]
        }
      ],
      "@typescript-eslint/ban-types": "warn",
      "@typescript-eslint/ban-ts-comment": "warn",
      "@typescript-eslint/prefer-as-const": "warn",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/no-parameter-properties": "off",
      "@typescript-eslint/prefer-interface": "off",
      "@typescript-eslint/explicit-member-accessibility": [
        "error",
        {
          "accessibility": "explicit",
          "overrides": {
            "accessors": "explicit",
            "constructors": "no-public",
            "methods": "explicit",
            "properties": "off",
            "parameterProperties": "explicit"
          }
        }
      ],
      "@typescript-eslint/no-shadow": [
        "error"
      ],
      "@typescript-eslint/no-non-null-assertion": "off"
    },
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    }
  }
}
