{
  "name": "modern-stack-portfolio-react-client",
  "version": "1.0.0",
  "private": false,
  "sideEffects": false,
  "main": ".next/server/index.js",
  "scripts": {
    "dev": "next",
    "build": "rm -fr .next && next build && yarn copy",
    "export": "next export",
    "copy": "node static/copy",
    "start": "next start",
    "test": "jest",
    "now-build": "yarn build",
    "next:build": "yarn build && yarn build:server",
    "docker:build": "IS_DOCKER=true DISABLE_SERVERLESS=true yarn build && yarn build:server",
    "build:server:only": "DISABLE_SERVERLESS=true NODE_ENV=production next build",
    "install:workspace": "cd ../../ && yarn install && cd packages/client",
    "prepublishOnly": "rm -rf build && tsc || true",
    "dev:webpack": "webpack-dev",
    "dev:storybook": "start-storybook -p 9001 -c .storybook",
    "git:push": "yarn autofix && yarn test",
    "fix:all": "yarn prettier && yarn styledlint",
    "fix:prettier": "prettier --write src/**/*.{ts,tsx} && prettier --write src/*.{ts,tsx}",
    "lint:style": "stylelint './src/**/*.(tsx|ts)' --config './stylelint.config.js'",
    "lint:ts": "tslint --format codeFrame --project tsconfig.json --config tslint.json",
    "build:babel": "NODE_ENV=development BABEL_ENV=development npx babel build/dist --out-dir build/dist || true",
    "test:dev": "jest --config jest.config.js",
    "build:server": "yarn build && yarn build:server:only && tsc || true && tsc --project tsconfig.server.json || true",
    "start:server": "DISABLE_SERVERLESS=true NODE_ENV=production node .next/server",
    "dev:server": "rm -fr dist && rm -fr .next && yarn build:server && yarn start:server"
  },
  "_resolutions": {
    "webpack": "5.0.0-alpha.11"
  },
  "__resolutions": {
    "node-fetch": "2.6.1",
    "fork-ts-checker-webpack-plugin": "3.1.0",
    "apollo-link": "1.2.11",
    "styled-components": "5.2.1"
  },
  "dependencies": {
    "next": "10.0.7",
    "next-server": "9.0.5",
    "@zeit/next-typescript": "^1.1.1",
    "next-offline": "5.0.3",
    "node-fetch": "2.6.1",
    "react-amphtml": "^4.0.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.1",
    "styled-components": "5.2.1",
    "express": "4.17.1",
    "react-router-dom": "5.2.0",
    "react-router": "5.2.0",
    "react-router-config": "5.1.1",
    "history": "5.0.0",
    "react-apollo": "3.1.5",
    "apollo-client": "2.6.10",
    "apollo-boost": "0.4.9",
    "apollo-link-state": "0.4.2",
    "apollo-cache-persist": "0.1.1",
    "pino": "6.11.0",
    "idb": "4.0.5",
    "idb-keyval": "3.2.0",
    "mkdirp": "1.0.4",
    "image-size": "0.9.3"
  },
  "devDependencies": {
    "web-vitals": "1.1.0",
    "raf": "3.4.1",
    "fork-ts-checker-webpack-plugin": "3.1.0",
    "lighthouse": "https://github.com/GoogleChromeLabs/lighthousebot",
    "@zeit/next-bundle-analyzer": "0.1.2",
    "danger": "10.6.2",
    "next-size": "2.1.0",
    "@types/gapi.client.tagmanager": "2.0.2",
    "intersection-observer": "0.12.0",
    "@types/lru-cache": "5.1.0",
    "@types/image-size": "0.8.0",
    "@types/mkdirp": "1.0.1",
    "schema-dts": "0.4.5",
    "pino-pretty": "4.5.0",
    "@types/pino": "6.3.5",
    "graphql": "15.5.0",
    "@types/graphql": "14.5.0",
    "@types/history": "4.7.8",
    "@types/react-router": "5.1.11",
    "@types/react-router-dom": "5.1.7",
    "@types/express": "^4.17.11",
    "@types/webpack-env": "^1.16.0",
    "typescript": "4.1.5",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/styled-components": "5.1.7",
    "babel-plugin-styled-components": "1.12.0",
    "stylelint": "13.10.0",
    "stylelint-processor-styled-components": "1.10.0",
    "@storybook/addon-storyshots": "^6.1.18",
    "@storybook/addon-knobs": "^6.1.18",
    "@storybook/react": "^6.1.18",
    "babel-loader": "8.2.2",
    "@babel/core": "7.12.16",
    "@babel/cli": "7.12.16",
    "@babel/plugin-syntax-jsx": "7.12.13",
    "@babel/plugin-syntax-object-rest-spread": "7.8.3",
    "@babel/plugin-proposal-decorators": "7.12.13",
    "@babel/preset-env": "7.12.16",
    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "ts-jest": "26.5.1",
    "jest-styled-components": "7.0.3",
    "@types/jest": "26.0.20",
    "@testing-library/react": "11.2.5",
    "ts-node": "9.1.1",
    "tslib": "2.1.0",
    "typescript-styled-plugin": "0.15.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "ts-loader": "8.0.17",
    "date-fns": "2.17.0"
  },
  "folders": [
    ".next",
    "server",
    "pages",
    "src"
  ],
  "license": "MIT",
  "description": "React client portion of the generic portfolio, written in the modern web stack, used on jameswiens.dev site",
  "homepage": "https://jameswiens.dev",
  "author": "James Wiens <james@jameswiens.dev> (https://jameswiens.dev)",
  "repository": {
    "type": "git",
    "url": "git@github.com:aretecode/modern-stack-portfolio-react.git"
  },
  "keywords": [
    "jameswiens",
    "james",
    "wiens",
    "dev",
    "aretecode",
    "portfolio",
    "react",
    "graphql",
    "typescript",
    "styled-components"
  ],
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "useTabs": false,
    "parser": "typescript",
    "trailingComma": "es5",
    "arrowParens": "avoid"
  }
}
